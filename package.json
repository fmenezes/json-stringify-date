{
  "name": "json-stringify-date",
  "version": "1.0.0",
  "description": "Like JSON.stringify, but preserve timezones in date objects and parse dates into Date object",
  "main": "index.js",
  "browser": "browser.js",
  "dependencies": {
    "moment": "^2.27.0"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.0",
    "browserify": "^16.5.1",
    "jshint": "^2.11.1",
    "mocha": "^2.2.1",
    "mocha-chrome": "^1.1.0",
    "nyc": "^13.3.0",
    "should": "^5.2.0"
  },
  "scripts": {
    "version": "npm run changelog && git add CHANGELOG.md",
    "preversion": "npm test",
    "postversion": "git push && git push --tag",
    "test": "npm run test:lint && npm run test:coverage && npm run test:build && npm run test:node && npm run test:browser",
    "test:coverage": "nyc npm run test:node",
    "test:node": "mocha",
    "test:browser": "mocha-chrome test/index.html",
    "test:lint": "jshint *.js **/*.js",
    "test:build": "./script/check.sh",
    "changelog": "auto-changelog -u --ignore-commit-pattern FUNDING",
    "build": "browserify --no-browser-field -e index.js -s JSONStringifyDate -o browser.js"
  },
  "repository": "git://github.com/fmenezes/json-stringify-date",
  "keywords": [
    "json",
    "stringify",
    "date",
    "parse"
  ],
  "author": "Filipe Constantinov Menezes <fcmenezes87@gmail.com>",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "lines": 95
  }
}
